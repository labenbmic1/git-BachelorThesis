\NeedsTeXFormat{LaTeX2e}%
\ProvidesExplPackage {packages/tumtikz} {\TUMversiondate} {\TUMversion}%
	{The tikz/pgf-part made by Michael Labenbacher}%
\ProcessKeysPackageOptions{packages/tumtikz}%
\ExplSyntaxOff%
\newlength{\tikZlengthwi}\setlength{\tikZlengthwi}{0.10mm}%
\newlength{\tikZlengthwii}\setlength{\tikZlengthwi}{0.14mm}%
\newlength{\tikZlengthwiii}\setlength{\tikZlengthwiii}{0.28mm}%
\newlength{\tikZlengthwiv}\setlength{\tikZlengthwiv}{0.56mm}%
\newlength{\tikZlengthwv}\setlength{\tikZlengthwv}{1.12mm}%
\newlength{\tikZlengthwvi}\setlength{\tikZlengthwvi}{2.24mm}%
\newlength{\tikZlengthwvii}\setlength{\tikZlengthwvii}{4.48mm}%
\tikzset{w1/.style={line width=\tikZlengthwi}}
\tikzset{w2/.style={line width=\tikZlengthwii}}
\tikzset{w3/.style={line width=\tikZlengthwiii}}
\tikzset{w4/.style={line width=\tikZlengthwiv}}
\tikzset{w5/.style={line width=\tikZlengthwv}}
\tikzset{w6/.style={line width=\tikZlengthwvi}}
\tikzset{w7/.style={line width=\tikZlengthwvii}}
\makeatletter%
\AtBeginDocument{%
	\TUM@ifpackageloaded[setting]{babel}{%
		\AfterEndPreamble{%
			\TUM@addto@extras{american,english}{%
				\pgfplotsset{%
					x tick label style={/pgf/number format/set decimal separator=.},%
					y tick label style={/pgf/number format/set decimal separator=.},%
				}%
			}%
			\TUM@addto@extras{naustrian,ngerman}{%
				\pgfplotsset{%
					x tick label style={/pgf/number format/set decimal separator={,}},%
					y tick label style={/pgf/number format/set decimal separator={,}},%
				}%
			}%
		}%
	}{}%
}%
\makeatother%
\pgfplotsset{compat=newest,%
	x tick label style={/pgf/number format/1000 sep={\;\!}},%
	y tick label style={/pgf/number format/1000 sep={\;\!}},%
	%% Wissenschaftliche Arbeiten mit geschlossenen Diagrammen und ausfuehrlicher Caption.
	TUM style 1/.append style={%
		tick style={%
			black, w1%
		},%
		tick align={%
			outside%
		},%
		major grid style={solid, black, w2},%
		minor grid style={dotted, black, w1},%
		inner axis line style={solid, black, w3},%
		axis line style={-stealth},%
		hide obscured x ticks=false, hide obscured y ticks=false%
	},%
	cycle list={
		{\TUMcolor{tikz-1}, mark=x},
		{\TUMcolor{tikz-2}, mark=diamond},
		{\TUMcolor{tikz-3}, mark=square},
		{\TUMcolor{tikz-4}, mark=star},
		{\TUMcolor{tikz-5}, mark=o},
		{\TUMcolor{tikz-6}, mark=triangle}},%
}%
\tikzset{>=latex}%
\makeatletter%
\newcommand{\gettikzxy}[3]{%
	\tikz@scan@one@point\pgfutil@firstofone#1\relax%
	\edef#2{\the\pgf@x}%
	\edef#3{\the\pgf@y}%
}%
\makeatother%
\endinput%
